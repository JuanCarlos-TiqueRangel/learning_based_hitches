<mujoco model="Cable">
  <include file="scene.xml"/>

  <extension>
    <plugin plugin="mujoco.elasticity.cable"/>
  </extension>

  <statistic center="0 0 .3" extent="1"/>
  <visual>
    <global elevation="-30"/>
  </visual>

  <compiler autolimits="true"/>

  <size memory="1000M"/>

  <worldbody>

    <!-- cable 1 -->
    <composite prefix="RA" type="cable" curve="s" count="100" size="1.0" offset="-.3 -0.5 1.">
      <plugin plugin="mujoco.elasticity.cable">
        <!-- Units are in Pa (SI) -->
        <config key="twist" value="1e5"/> <!-- Twist stiffness: Increased twist stiffness for more rigidity -->
        <config key="bend" value="1e5"/> <!-- Bend stiffness: Reduced bend stiffness for more flexibility -->
        <config key="vmax" value="0.0"/> <!-- Maximum velocity -->
      </plugin>
      <!-- <joint kind="main" damping=".015"/> -->
      <joint kind="main" damping="0.01"/>
      <geom type="capsule" size="0.005" rgba=".8 .2 .1 1" condim="1"/>
    </composite>

    <!-- red point body -->    
    <body name="A_point" pos=".7 -0.5 1.">
      <!-- 3 degrees of freedom -->
      <joint name="A_point_x" type="slide" axis="1 0 0" damping=".1"/>
      <joint name="A_point_y" type="slide" axis="0 1 0" damping=".1"/>
      <joint name="A_point_z" type="slide" axis="0 0 1" damping=".1"/>

      <!-- Visual red sphere -->
      <geom name="A_sphere" type="sphere" size="0.02" rgba="1 0 0 1"/>
      
    </body>


    <!-- cable 2 -->
    <composite prefix="RB" type="cable" curve="s" count="100" size="1.0" offset="-.3 0.5 1." initial="none">
      <plugin plugin="mujoco.elasticity.cable">
        <!--Units are in Pa (SI)-->
        <config key="twist" value="1e5"/> <!-- Twist stiffness: Increased twist stiffness for more rigidity -->
        <config key="bend" value="1e5"/> <!-- Bend stiffness: Reduced bend stiffness for more flexibility -->
        <config key="vmax" value="0.0"/> <!-- Maximum velocity -->
      </plugin>
      <!-- <joint kind="main" damping=".015"/> -->
      <joint kind="main" damping="0.01"/>
      <geom type="capsule" size="0.005" rgba=".8 .2 .1 1" condim="1"/>
    </composite>

    <!-- red point body -->    
    <body name="B_point" pos=".7 0.5 1.">
      <!-- 3 degrees of freedom -->
      <joint name="B_point_x" type="slide" axis="1 0 0" damping=".1"/>
      <joint name="B_point_y" type="slide" axis="0 1 0" damping=".1"/>
      <joint name="B_point_z" type="slide" axis="0 0 1" damping=".1"/>

      <!-- Visual red sphere -->
      <geom name="B_sphere" type="sphere" size="0.02" rgba="1 0 0 1"/>
    </body>

  </worldbody>


  <!-- Rigidly attach each cable tip to its drone -->
  <equality>
    <!-- <weld name="attach_RA" body1="RAB_last" body2="A_point" solref="0.002 1"/>
    <weld name="attach_RB" body1="RBB_last" body2="B_point" solref="0.002 1"/> -->
    <connect name="cableA_to_A" body1="RAB_last" body2="A_point" anchor="0 0 0"/>
    <connect name="cableB_to_B" body1="RBB_last" body2="B_point" anchor="0 0 0"/>
  </equality>

  <!-- Avoid self-collisions at the attachment -->
  <contact>
    <exclude body1="RAB_last" body2="A_point"/>
    <exclude body1="RBB_last" body2="B_point"/>
  </contact>


  <actuator>
    <!-- Position actuators on each axis -->
    <position name="RA_x" joint="A_point_x" kp="400.0" kv="38.5"/>
    <position name="RA_y" joint="A_point_y" kp="400.0" kv="38.5"/>
    <position name="RA_z" joint="A_point_z" kp="400.0" kv="38.5"/>
  </actuator>

  <actuator>
    <!-- Position actuators on each axis -->
    <position name="RB_x" joint="B_point_x" kp="400.0" kv="38.5"/>
    <position name="RB_y" joint="B_point_y" kp="400.0" kv="38.5"/>
    <position name="RB_z" joint="B_point_z" kp="400.0" kv="38.5"/>
  </actuator>

